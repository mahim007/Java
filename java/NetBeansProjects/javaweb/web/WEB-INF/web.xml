<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>helloservlet</servlet-name>
        <servlet-class>helloservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>studentformservlet</servlet-name>
        <servlet-class>studentformservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newstudentformservlet</servlet-name>
        <servlet-class>newstudentformservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jdbctest</servlet-name>
        <servlet-class>jdbctest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>easyservlet</servlet-name>
        <servlet-class>easyservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>helloservlet</servlet-name>
        <url-pattern>/helloservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>studentformservlet</servlet-name>
        <url-pattern>/studentformservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newstudentformservlet</servlet-name>
        <url-pattern>/newstudentformservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jdbctest</servlet-name>
        <url-pattern>/jdbctest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>easyservlet</servlet-name>
        <url-pattern>/easyservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
